								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "arrayvalue",
								"value": {
									"value": "@range(int(split(item().OrderUnits, '..')[0])\n, add(sub(int(split(item().OrderUnits, '..')[1]), int(split(item().OrderUnits, '..')[0])), 1))",
									"type": "Expression"
								}
							}
						}
					]
				}
			},
			{
				"name": "Script1",
				"type": "Script",
